{
  "name": "beetle-ui",
  "version": "0.1.3",
  "description": "React component library",
  "author": "Censwin",
  "license": "MIT",
  "keywords": [
    "ui",
    "react",
    "component"
  ],
  "homepage": "https://github.com/Censwin/beetle-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/Censwin/beetle-ui"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "dist/index.d.ts",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "axios": "^0.23.0",
    "classnames": "^2.3.1",
    "cra-template-typescript": "1.1.2",
    "react-is": "^16.13.1",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-ts && npm run build-css",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 10",
    "lint_fix": "eslint '{components,example}/**/*.{ts,tsx,js,jsx}' --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o ./docs",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "create-keyframe-animation": "^0.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^26.6.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.3",
    "web-vitals": "^2.1.1"
  }
}
